    "react-plaid-link": "^3.6.1",
    "react-router-dom": "^7.3.0",
    "react-turnstile": "^1.1.4",
    "readline": "^1.3.0",
    "recharts": "^2.12.0",
    "rimraf": "^6.0.1",
    "stripe": "^17.7.0",
