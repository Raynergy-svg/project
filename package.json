{
  "name": "smart-debt-flow",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.20.3"
  },
  "scripts": {
    "predev": "node scripts/check-node-version.js",
    "prebuild": "node scripts/check-node-version.js",
    "dev": "vite --port 5173",
    "server": "node start-server.js",
    "server:dev": "node start-server.js",
    "proxy": "node proxy-server.js",
    "start-local": "node start-local.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "prebuild-with-verify": "node scripts/verify-dependencies.js",
    "build": "npm run clean-debug && tsc && vite build",
    "build:ci": "npm install && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage && open coverage/index.html",
    "test:integration": "vitest run src/tests/integration/*.test.ts{,x}",
    "test:unit": "vitest run src/tests/unit/*.test.ts{,x}",
    "test:components": "vitest run src/tests/unit/components/**/*.test.ts{,x}",
    "test:journeys": "vitest run src/tests/journeys/*.test.ts{,x}",
    "test:error-handling": "vitest run src/tests/journeys/error-handling.test.ts{,x}",
    "test:performance": "vitest run src/tests/performance/*.test.ts{,x}",
    "test:security": "vitest run src/tests/security/*.test.ts{,x}",
    "test:security:auth": "vitest run src/tests/security/auth-security.test.ts",
    "test:security:api": "vitest run src/tests/security/api-security.test.ts",
    "test:security:data": "vitest run src/tests/security/data-protection.test.ts",
    "test:security:all": "npm run test:security:auth && npm run test:security:api && npm run test:security:data",
    "test:e2e": "cypress run",
    "test:ci": "vitest run --coverage && npm run test:security",
    "test:auth": "node scripts/test-auth.cjs",
    "test:auth:watch": "node scripts/test-auth.cjs --watch",
    "test:auth:ui": "node scripts/test-auth.cjs --ui",
    "analyze": "vite-bundle-analyzer",
    "clean-debug": "node scripts/clean-debug.js",
    "restore-debug": "node scripts/restore-debug.js",
    "create-investors": "node scripts/create-investor-accounts.js",
    "test:supabase": "ts-node -r tsconfig-paths/register src/tests/run-supabase-check.ts",
    "test:db-schema": "vitest run src/tests/db-schema.test.ts",
    "test:db": "npm run test:supabase && npm run test:db-schema",
    "test:rls": "ts-node -r tsconfig-paths/register src/tests/run-rls-check.ts",
    "test:security-check": "ts-node -r tsconfig-paths/register src/tests/run-security-check.ts",
    "# DEV ONLY - Database migration scripts": "These scripts are for development only and should not be used in production",
    "db:migration:create": "node scripts/db-migration.js create",
    "db:migrate:local": "node scripts/db-migration.js migrate local",
    "db:migrate:staging": "node scripts/db-migration.js migrate staging",
    "db:migrate:prod": "node scripts/db-migration.js migrate production",
    "db:rollback:local": "node scripts/db-migration.js rollback local",
    "db:rollback:staging": "node scripts/db-migration.js rollback staging",
    "db:rollback:prod": "node scripts/db-migration.js rollback production",
    "db:status": "node scripts/db-migration.js status",
    "db:scan-security": "node scripts/scan-security-issues.js"
  },
